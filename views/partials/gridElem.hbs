<div class="grid-item" name={{this.id}}>
  <div class="imageHolder">
    <img class="mainImage" src={{this.link}} alt={{this.title}}>
    <p>{{this.title}}</p>
  </div>
  <div class="data">
    <a href="/{{this.owner}}" ><img id="profile" src={{this.profileLink}} title="{{this.owner}}" alt={{this.owner}}></a>
  {{#if this.own}}
       <i id="{{this.id}}" class="fa fa-times delete" aria-hidden="true" ></i>   
  {{/if}}  
    <div data-id={{this.id}} data-voted={{voted}} class="like btn btn-default btn-sm" >
      <span><i class="fa fa-thumbs-up" aria-hidden="true"></i></span>
      <span class="likes" >{{this.likes}}</span>
    </div>
  </div>
</div>






